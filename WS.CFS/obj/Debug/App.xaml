<Application
    x:Class="WS.CFS.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WS.CFS"
    xmlns:localData ="using:WS.CFS.Data"
    RequestedTheme="Dark">

    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="AppName">Citizen Feedback CRM System</x:String>
            <localData:FeedbackDataSource x:Key="feedbackDataSource" />
        </ResourceDictionary>
    </Application.Resources>
    
</Application>

